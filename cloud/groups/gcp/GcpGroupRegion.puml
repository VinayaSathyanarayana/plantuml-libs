!endfunction
skinparam Rectangle<<GcpGroupRegion>> {
  StereotypeFontColor $GCP_GROUP_FONT_COLOR
  BorderThickness 0
  BorderColor none
  BorderStyle solid
  FontStyle none
  FontColor $GCP_GROUP_FONT_COLOR
