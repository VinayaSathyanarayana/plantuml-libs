!endprocedure
skinparam Rectangle<<GcpGroupPod>> {
  StereotypeFontColor $GCP_GROUP_FONT_COLOR
  BorderThickness 0
  BorderColor none
  BorderStyle solid
  FontStyle none
  FontColor $GCP_GROUP_FONT_COLOR
  FontSize $FONT_SIZE_MD
  BackgroundColor #E8F5E9
  shadowing false
}
@enduml
