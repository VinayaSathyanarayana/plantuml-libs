    "@types/node": "^18.18.5",
    "@types/node-fetch": "^2.6.6",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "csv-parse": "^5.0.4",
    "eslint": "^8.38.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.1.1",
    "html-minifier-terser": "^7.0.0",
    "lodash": "^4.17.21",
    "marked": "^9.1.2",
    "marked-gfm-heading-id": "^3.0.3",
    "marked-mangle": "^1.0.1",
    "mocha": "^10.0.0",
    "prettier": "^3.0.3",
    "standard-version": "^9.3.2",
    "ts-node": "^10.4.0",
    "typescript": "^5.0.3",
    "yaml": "^2.1.1"
  },
  "dependencies": {
    "glob": "^10.2.5",
    "moment": "^2.29.1",
    "node-fetch": "^2.7.0",
    "yargs": "^17.2.1"
