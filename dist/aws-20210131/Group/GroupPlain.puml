@startuml
!procedure GroupPlain($id, $name='Group Plain', $tech='')
  IconGroup($id, 'GroupPlain', '<$Lg>', $name, $tech)
!endprocedure
skinparam Rectangle<<GroupPlain>> {
  StereotypeFontColor $AWS_COLOR_DARK_BLUE
  BorderThickness 0
  BorderColor none
  BorderStyle solid
  FontStyle none
  FontColor $AWS_COLOR_DARK_BLUE
  FontSize $FONT_SIZE_MD
  BackgroundColor $AWS_COLOR_GREY_BG
  shadowing false
}
@enduml