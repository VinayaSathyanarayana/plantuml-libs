@startuml
!procedure GroupDashed($id, $name='Group Dashed', $tech='')
  IconGroup($id, 'GroupDashed', '<$Lg>', $name, $tech)
!endprocedure
skinparam Rectangle<<GroupDashed>> {
  StereotypeFontColor $AWS_COLOR_GREY
  BorderThickness 1
  BorderColor $AWS_COLOR_GREY
  BorderStyle dashed
  FontStyle none
  FontColor $AWS_COLOR_GREY
  FontSize $FONT_SIZE_MD
  BackgroundColor transparent
  shadowing false
}
@enduml