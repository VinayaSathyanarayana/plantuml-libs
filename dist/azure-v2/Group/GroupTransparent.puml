@startuml
!procedure GroupTransparent($id, $name='Group Transparent', $tech='')
  IconGroup($id, 'GroupTransparent', '<$Lg>', $name, $tech)
!endprocedure
skinparam Rectangle<<GroupTransparent>> {
  StereotypeFontColor $FONT_COLOR
  BorderThickness 0
  BorderColor transparent
  BorderStyle solid
  FontStyle none
  FontColor $FONT_COLOR
  FontSize $FONT_SIZE_MD
  BackgroundColor transparent
  shadowing false
}
@enduml