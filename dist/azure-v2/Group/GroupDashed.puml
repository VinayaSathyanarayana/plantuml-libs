@startuml
!procedure GroupDashed($id, $name='Group Dashed', $tech='')
  IconGroup($id, 'GroupDashed', '<$Lg>', $name, $tech)
!endprocedure
skinparam Rectangle<<GroupDashed>> {
  StereotypeFontColor $FONT_COLOR
  BorderThickness 1
  BorderColor $AZURE_GROUP_BD_DARK
  BorderStyle dashed
  FontStyle none
  FontColor $FONT_COLOR
  FontSize $FONT_SIZE_MD
  BackgroundColor transparent
  shadowing false
}
@enduml